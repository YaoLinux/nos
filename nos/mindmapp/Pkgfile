description="Desktop application to draw mind maps"
url="https://mindmapp.org"

packager="Yaolinux Team"

name=mindmapp
version=0.7.16

source=(https://github.com/Mindmapp/mindmapp/releases/download/v${version}/Mindmapp-${version}.deb)

build() {

tar xvf data.tar.xz
cp -r opt $PKG
cp -r usr $PKG
}
