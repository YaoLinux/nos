makedepends=(openjdk xdg-utils)

description="Cross-platform file backup"
url="http://www.frostwire.com"

packager="Yaolinux Team"
maintainer="rems"

name=frostwire
version=6.8.9

run=(openjdk)

source=(https://prime.frostwire.com/frostwire/$version/$name-$version.amd64.deb)

build() {

tar xvf data.tar.xz -C $PKG
}

